{
    "name": "qiku/zhuti-api",
    "description": "zhuti api project for qiku corporation",
    "keywords": ["internal", "framework", "laravel", "lumen", "zhuti"],
    "license": "MIT",
    "type": "project",
    "homepage": "http://api.zhuti.qiku.com/",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "predis/predis": "^1.0",
        "illuminate/redis": "~5.1",
        "data/verify": "1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/yooshang/verify"
        }
    ],
}
